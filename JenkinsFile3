pipeline {
    agent any
    
    environment{
        PATH = "/opt/apache-maven-3.8.3/bin:$PATH"
    }
    stages {
        stage('Git Checkout') {
            steps {
                git 'https://github.com/gyan-ripu/myweb'
            }
        }
        stage('Maven Build') {
            steps {
                sh "mvn clean package"
            }
        }
    }
}
